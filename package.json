{
  "name": "bakery-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "amazon-cognito-identity-js": "^1.21.0",
    "antd": "^3.3.1",
    "aws-sdk": "^2.141.0",
    "axios": "^0.17.1",
    "bulma": "^0.6.2",
    "cra-append-sw": "^2.2.0",
    "crypto-js": "^3.1.9-1",
    "google-maps-react": "^1.1.2",
    "husky": "^0.14.3",
    "instafeed.js": "^1.4.1",
    "jquery": "^3.2.1",
    "lint-staged": "^7.2.0",
    "localforage": "^1.6.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.13.5",
    "react": "^16.0.0",
    "react-animations": "^0.1.0",
    "react-dom": "^16.0.0",
    "react-lazy-load": "^3.0.13",
    "react-progressive-bg-image": "^2.1.1",
    "react-responsive": "^4.0.3",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.0.14",
    "react-time": "^4.3.0",
    "styled-components": "^3.1.6"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-app-rewired start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-app-rewired build",
    "cra-append-sw": "cra-append-sw ./service-worker.js",
    "build": "npm-run-all build-css build-js cra-append-sw",
    "test": "react-app-rewired test --env=jsdom",
    "predeploy": "yarn run build",
    "deploy": "aws s3 sync build/ s3://vse-bulochki-tut",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id EDW4ZEX7KR7BT --paths '/*' && aws cloudfront create-invalidation --distribution-id E2PED889VH1U9N --paths '/*'",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.6.2",
    "react-app-rewire-less": "^2.0.7",
    "react-app-rewired": "^1.3.3",
    "react-dev-utils": "^4.1.0",
    "react-error-overlay": "^2.0.2"
  }
}
